<?xml version="1.0" encoding="UTF-8"?>
<database>
    <element label="Site Operations" max_length="40" name="x_snc_trip_advisor_site_operations" type="collection">
        <element label="Cache Discovery Last Update" max_length="40" name="cache_discovery_last_update" read_only="true" type="glide_date_time"/>
        <element label="Cache Discovery Places Count" max_length="40" name="cache_discovery_places_count" read_only="true" type="integer"/>
        <element label="Cache Discovery Request Count" max_length="40" name="cache_discovery_request_count" read_only="true" type="integer"/>
        <element label="Discovery Comments" mandatory="true" max_length="1000" name="discovery_comments" type="string"/>
        <element choice="1" label="GoogleMaps request type" max_length="40" name="googlemaps_request_type" type="choice">
            <choice>
                <element hint="radarsearch returns place_id and location only (200 max)" label="radarsearch - radarsearch returns place_id and location only (200 max)" sequence="100" value="getRadarsearch"/>
                <element hint="nearbysearch returns establishment details (60 max)" label="nearbysearch -nearbysearch returns establishment details (60 max)" sequence="200" value="getPlaceNearbysearch"/>
                <element hint="details returns details on a specifice place_id (max 1)" label="details -details returns details on a specifice place_id (max 1)" sequence="300" value="getDetails"/>
            </choice>
        </element>
        <element label="GoogleMaps URL" max_length="200" name="googlemaps_url" type="url"/>
        <element label="Location" max_length="40" name="location" type="string"/>
        <element label="Next page token" max_length="1000" name="next_page_token" type="string"/>
        <element attributes="ignore_filter_on_new=true" default="javascript:global.getNextObjNumberPadded();" label="Number" max_length="40" name="number" read_only="true" type="string"/>
        <element label="Place Id" max_length="40" name="place_id" type="string"/>
        <element default="5000" label="Radius" max_length="40" name="radius" type="string"/>
        <element label="Requests Today" max_length="40" name="requests_today" read_only="true" type="integer"/>
        <element label="Result" max_length="40" name="result" read_only="true" type="string"/>
        <element label="Returned Raw Data" max_length="100000" name="returned_raw_data" type="string"/>
        <element label="Type" max_length="40" name="type" type="string"/>
        <element label="Use location from googlemaps URL" max_length="40" name="use_location_from_googlemaps_url" type="boolean"/>
    </element>
</database>
